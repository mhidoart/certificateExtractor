function disableMoments(){var now=getDateTime();var d=getDateNow(now);var h=now.getHours();var holidays=$('#product_form_date option:selected').data('holidays');var isMothersDayPeriod=$('#product_form_date option:selected').data('fdm');$('#product_form_moment :radio').each(function(index){if(index!==0){if(holidays===1||isMothersDayPeriod===1||$('#product_form_moment').data('lock')===1){$(this).prop('disabled',true).addClass('disabled');$('#product_form_moment :radio[value="0"]').prop('checked',true);}else{$(this).prop('disabled',false).removeClass('disabled');var weekend=$.inArray(now.getDay(),[0,6])!==-1;var choice=new Date($('#product_form_date').val());var dayDiff=Math.ceil((choice.getTime()-now.getTime())/(1000*3600*24));if($('#product_form_date').val()==d&&h>=11||(weekend&&choice.getDay()==1&&dayDiff<=3)){$('#product_form_moment :radio[value="0"]').prop('checked',true);$('#product_form_moment :radio[value="2"]').prop('disabled',true);}}}});}
function displayPrice(priceText){if(typeof priceText==='string'){priceText=priceText.replace(',','.');}
var price=parseFloat(priceText);$.each($('.accessory.active input'),function(){price+=parseFloat($(this).data('price'));});$('#tot').text(price.toFixed(2).replace(".",","));}
function calculMaximiles(prix){if(isNaN(parseInt(prix))){prix=$('#tot').text();}
if(typeof prix=='string'){prix=prix.replace(',','.');}
if(!isNaN(parseInt(prix))){var $pm=$('#points_maximiles');var total=parseFloat($pm.text())+parseFloat(prix)*2;$pm.text(total<0?0:Math.round(total));}}
function getDateTime(){return new Date("2020/12/04 14:07:11");}
function getDateNow(now){var month=now.getMonth()+1;month=month>=10?month:'0'+month;var day=now.getDate();day=day>=10?day:'0'+day;return now.getFullYear()+'-'+month+'-'+day;}
function recalculProductPage($formule){var $selectOpt=$('.accessory');var $radio=$formule.find('input[type="radio"]');if($radio.length>0){var $dga=$('.declinaison-group').find('.active');$dga.find('input[type="radio"]').prop("checked",false);$dga.removeClass("active");$radio.prop("checked",true);$radio.change();$formule.addClass('active');if(!$formule.hasClass('free-price')){var freePriceRadio=$('.free-price').find('input[type="radio"]');freePriceRadio.data('price',$radio.data('price'));freePriceRadio.attr('data-price',$radio.data('price'));freePriceRadio.attr('value',$radio.attr('value'));}
displayPrice($radio.data('price'));calculMaximiles('-'+$('#old_price').text());calculMaximiles($formule.find(".price").text());$.each($selectOpt,function(){var $checkBox=$(this).find('input[type="checkbox"]');if($checkBox.length>0){if($checkBox.is(":checked")){calculMaximiles($checkBox.data('price'));}}});var priceProduct=$formule.find(".price").attr('content');$('.product-accessory').each(function(){$(this).text(number_format((parseFloat($(this).data('accessory-price'))+parseFloat(priceProduct)),2,',','.'));});$('.product-bundle').each(function(){var url=$(this).data('action');$(this).data('action',updateURLParameter(url,'type',$radio.data('type')));});}}
function accessoriesNumber($input,$button,$totalChoise,$max){if($button.hasClass('plus')&&$totalChoise<$max){$input.val(parseInt($input.val())+1);progress('#product_form');$signe="+";}
if($button.hasClass('minus')&&$input.val()>0){$input.val(parseInt($input.val())-1);progress('#product_form');$signe="-";}
var priceProduct=$input.parent(".btn-group").siblings(".price").attr('content');recalculPrix();}
function recalculPrix(){$actualTotalPrice=parseFloat($('#tot').data('productprice'));$('.accessory').each(function(e){$accessoryPrice=0;$accessoryPrice+=($(this).find('.price').attr('content')*$(this).find('input').val());$actualTotalPrice+=$accessoryPrice;});if(!isNaN($actualTotalPrice)){$('#tot').text(number_format($actualTotalPrice,2,',','.'));calculMaximiles('-'+$('#old_price').text());calculMaximiles($actualTotalPrice);}}
function disableButtonsAccessories($input){var $totalChoose=0;$('.accessory:not(.option) .btn-group input').each(function(){$totalChoose+=parseInt(this.value);});var $buttonsPlus=$('.accessory:not(.option) .btn-group .plus');var $buttonsMinus=$('.accessory:not(.option) .btn-group .minus');disableButtons($buttonsPlus,$buttonsMinus,$totalChoose,$maxTotalChoose,$input);}
function disableButtonsOptions($input){var $buttonsPlus=$input.siblings('.plus');var $buttonsMinus=$input.siblings('.minus');disableButtons($buttonsPlus,$buttonsMinus,$input.val(),$maxTotalChooseOption,$input);}
function disableButtons($buttonsPlus,$buttonsMinus,$totalChoose,$max,$input){if($input!==undefined){if($input.val()>0){$input.siblings('.plus').attr('disabled',false);$input.siblings('.minus').attr('disabled',false);}else{$input.siblings('.minus').attr('disabled',true);}}
var $accWarning=$('#product_form .accessory-warning');if($totalChoose>=$max){$buttonsPlus.attr('disabled',true);if($input!==undefined){$.ajax({url:'/ajax/accessory/warning/'+$input.parent().parent().data('name'),method:'GET',success:function($response){if($response.warning){$($response.warning).appendTo($accWarning);}}});}}else{$buttonsPlus.attr('disabled',false);if($input!==undefined){var $name=$input.parent().parent().data('name');if($name!==undefined){$('.accessory-warning span[data-name="'+$input.parent().parent().data('name')+'"]').remove();}else{$('.accessory-warning span:not([data-name])').remove();}}}}</script><script>window.addEventListener("load",function(){checkDeliveryDateSelected();$("#product_form_date").on('change',function(){checkDeliveryDateSelected();});});function checkDeliveryDateSelected(){var $oS=$("#product_form_date").find("option:selected");var m=$oS.data('msg');if($oS.data('fdm')===1){var $mMD=$("#modal_mothers_day");$mMD.find(".fdm-message").text(m+".");$mMD.modal('show');}else if(m!==''&&m!==undefined){var $mM=$("#modal_msg");$mM.find(".modal-body").html(m);$mM.modal('show');}}</script><script>window.addEventListener("load",function(){$('#to_delivery_infos').on('click touch',function(e){e.preventDefault();var pos=$($(this).attr('href')).offset().top;pos-=100;$('html, body').animate({scrollTop:pos},500);})});</script><script>if(!!window.performance&&window.performance.navigation.type===2){location.reload(true);}</script><script>window.addEventListener("load",function(){$.ajax({url:"/api/init?levier=sem&gclid=Cj0KCQiA2af-BRDzARIsAIVQUOcdREA_edufIKprluQ1wGPQ4Xpot0ATxe0kmGmH1zaFoApEX-XViSoaAmi-EALw_wcB&opm=2871&utm_source=google&utm_medium=shopping",method:"GET",context:$(this)});});</script><script>window.addEventListener('load',function(){var aside=$('aside > .sticky-top');var window_h=$(window).height()-90;var scroll=$(this).scrollTop();changeStickyAside();$(document).scroll(function(){window_h=$(window).height()-90;scroll=$(this).scrollTop();changeStickyAside();});function changeStickyAside(){if(aside.height()>window_h){var diff_size=aside.height()-window_h;var i=$(this).scrollTop();if(i<diff_size){aside.css('top','-'+i+'px');}else{aside.css('top','-'+diff_size+'px');}}else{aside.css('top','4.5rem');}}});</script></body></html>